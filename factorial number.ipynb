   ],
   "source": [
    "num = int(input(\"enter any number: \"))\n",
    "fact = 1\n",
    "if num < 0:\n",
    "    print(\"Can't calculate factorial for -ive number's\")\n",
    "else:\n",
    "    while(num>=0):\n",
    "        fact *=num\n",
    "        num -= 1\n",
    "    print(fact)"
   ]
